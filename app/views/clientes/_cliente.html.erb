<div id="<%= dom_id cliente %>">
  <p>
    <strong>Nome:</strong>
    <%= cliente.nome %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= cliente.email %>
  </p>

  <p>
    <strong>Bairro:</strong>
    <%= cliente.bairro %>
  </p>

  <p>
    <strong>Rua:</strong>
    <%= cliente.rua %>
  </p>

  <p>
    <strong>Veterinario:</strong>
    <%= cliente.veterinario.nome %>
  </p>

  <p>
    <strong>Pet:</strong>
    <%= cliente.pet.nome %>
  </p>

</div>
